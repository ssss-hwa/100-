package dkssdu;

import java.util.*;
public class sdsd{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		
		int b = a/1000 ;
		int c = a%1000/100 ;
		int d = a%1000%100/10 ;
		int e = a%1000%100%10 ;
		
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		
				
		
		
		
	}

}
